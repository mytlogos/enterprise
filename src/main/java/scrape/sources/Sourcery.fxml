    <TextField fx:id="matchField" layoutX="262.0" layoutY="34.0"/>
    <Button fx:id="doItBtn" layoutX="435.0" layoutY="34.0" mnemonicParsing="false" onAction="#getPost"
            text="Button"/>
    <Label fx:id="title" layoutX="14.0" layoutY="90.0" text="Label"/>
    <Label fx:id="content" layoutX="14.0" layoutY="169.0" text="Label"/>
    <Label fx:id="footer" layoutX="14.0" layoutY="322.0" text="Label"/>
    <ComboBox fx:id="postsBox" layoutX="420.0" layoutY="65.0" onAction="#displayPost" prefWidth="150.0"/>
</AnchorPane>
