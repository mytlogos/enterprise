<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="Enterprise.test.TableViewer">
    <center>
        <AnchorPane fx:id="pane" prefHeight="404.0" prefWidth="566.0">
            <children>
                <TextField fx:id="name" layoutX="14.0" layoutY="361.0" prefHeight="25.0" prefWidth="313.0"
                           promptText="URL"/>
                <Button fx:id="addUrl" layoutX="337.0" layoutY="361.0" mnemonicParsing="false" onAction="#addInternet"
                        text="Bild runterladen"/>
                <Button fx:id="addPC" layoutX="450.0" layoutY="361.0" mnemonicParsing="false" onAction="#addLocal"
                        text="PC durchsuchen"/>
                <ImageView fx:id="image" fitHeight="340.0" fitWidth="533.0" layoutX="14.0" layoutY="14.0"
                           pickOnBounds="true" preserveRatio="true"/>
                <Button fx:id="show" layoutX="475.0" layoutY="329.0" mnemonicParsing="false" onAction="#showSideBar"
                        text="Button"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
